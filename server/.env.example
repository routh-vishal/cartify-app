# Database Configuration
DB_HOST=localhost                # Host for the database (usually 'localhost' for local development)
DB_PORT=5432                     # Port where your database is running (default for PostgreSQL is 5432)
DB_NAME=your_database            # The name of your database
DB_USER=your_user                # The username for your database
DB_PASSWORD=your_password        # The password for your database

# Server Configuration
PORT=5000                        # Port where your server will run (change to your preferred port)

# JWT Configuration
JWT_SECRET=your-jwt-secret-key   # Secret key used for signing JWT tokens (use a strong key)
